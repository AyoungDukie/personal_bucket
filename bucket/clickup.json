            "ClickUp"
        ]
    ],
    "autoupdate": {
        "url": "https://desktop.clickup.com/windows/Clickup Setup $version-x64.exe",
        "hash": {
            "url": "$baseurl/latest.yml",
            "regex": "sha512:\\s+$base64"
        }
    }
