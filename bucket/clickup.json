            "installer": {
                "file": "Clickup Setup 3.0.1-x64.exe",
                "args": [
                    "/S",
                    "/currentuser",
                    "/disableAutoUpdates",
                    "/D=$dir"
                ],
            },
            "uninstaller": {
                "file": "Uninstall ClickUp.exe",
                "args": "/S"
            }
        }
